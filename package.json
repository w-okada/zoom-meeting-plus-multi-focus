{
    "name": "zoom-meeting-plus",
    "version": "1.0.0",
    "description": "",
    "main": ".eslintrc.js",
    "scripts": {
        "clean": "rimraf dist/*",
        "copy:resources": "mkdir -p dist && cp -r public/* dist/",
        "tsc": "tsc",
        "tsc:watch": "tsc -w",
        "webpack:build": "webpack --config webpack.config.js",
        "webpack:start": "webpack serve --config webpack.config.js",
        "build": "run-s clean copy:resources tsc webpack:build ",
        "build:docker": "date +%Y%m%d%H%M%S > dummy && DOCKER_BUILDKIT=1 docker build --ssh default  . -t zoom-meeting-plus",
        "start:backend": "node backend/server.js",
        "start": "run-p copy:resources tsc:watch webpack:start start:backend",
        "start:voicevox": "docker run --rm -it -p '127.0.0.1:50021:50021' voicevox/voicevox_engine:cpu-ubuntu20.04-latest",
        "deploy:backend": "git push heroku master",
        "heroku-postbuild": "echo \"nobuild in heroku(w.o.)\"",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/w-okada/zoom-meeting-plus.git"
    },
    "keywords": [],
    "author": "",
